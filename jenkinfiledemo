pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
            
        }
        stage('Build') {
            steps {
                echo 'Hello am Building a pipeline'
            }
        }
        stage('Deploye') {
            steps {
                echo 'Hello am going to Deploying a code'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello am Testing your pipeline'
            }
        }
        stage('Release') {
            steps {
                echo 'Hello am going to Releasing you in this sprint'
            }
        }
        
    }
}
